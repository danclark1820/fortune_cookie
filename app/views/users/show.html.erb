<h3>
  <%= @user.email %>
</h3><br>
  <% @notes.each do |n| %>
    <div id="date">
      <%= n.date_created %>
    </div>
    <div id="fortune">
      <%= n.fortune[0].fortune %><br>
      -<%= n.fortune[0].author %>
    </div><br>
    <div id="note">
      <%= simple_format(n.body) %>
    </div><br><br>
  <% end %>
